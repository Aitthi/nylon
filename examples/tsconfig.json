{
    "compilerOptions": {
      "target": "ES2020",
      "strict": true,
      "moduleResolution": "node",
      "module": "CommonJS",
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "types": [
        "node"
      ],
      "declaration": true
    },
    "include": [
      "main.ts"
    ]
  }